{
  "name": "aureum-desktop",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
  "dev:web": "vite",
  "dev:desktop": "concurrently -k \"npm:dev:web\" \"npm:electron\"",
  "electron": "wait-on http://localhost:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron ./electron/main.cjs",
  "build": "tsc -b && vite build",
  "build:desktop": "npm run build && electron-builder",
  "start:desktop": "cross-env NODE_ENV=production electron ./electron/main.cjs",
  "lint": "eslint .",
  "test": "vitest",
  "preview": "vite preview"
},

  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@iconify/react": "^6.0.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@react-oauth/google": "^0.12.2",
    "@supabase/supabase-js": "^2.81.1",
    "@tanstack/react-query": "^5.90.11",
    "@vercel/speed-insights": "^1.3.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dotenv": "^17.2.3",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "motion": "^12.23.24",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.66.1",
    "react-i18next": "^16.2.1",
    "react-router-dom": "^7.9.4",
    "recharts": "^3.5.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "yup": "^1.7.1",
    "zod": "^4.1.12",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "jsdom": "^27.3.0",
    "postcss": "^8.5.6",
    "sass-embedded": "^1.93.2",
    "tailwindcss": "3",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.2.2",
    "vitest": "^4.0.15",
    "wait-on": "^9.0.3"
  },
  "build": {
    "appId": "com.aureum.desktop",
    "productName": "Aureum Desktop",
    "directories": {
      "buildResources": "electron/assets",
      "output": "dist_electron"
    },
    "files": [
      "dist/**",
      "electron/**",
      "package.json"
    ],
    "win": {
      "target": "nsis",
      "icon": "electron/assets/icon.ico"
    }
  }
}
